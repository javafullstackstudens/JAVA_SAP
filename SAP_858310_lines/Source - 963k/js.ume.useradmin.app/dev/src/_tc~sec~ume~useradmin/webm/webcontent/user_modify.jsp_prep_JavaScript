   inputTag2.setAttribute("name", "Cx_0dummy");
   inputTag2.setAttribute("type", "hidden");
   inputTag2.setAttribute("value", "Cx_0dummy");
   frm.appendChild( inputTag2 );
   frm.submit();
 }
 
