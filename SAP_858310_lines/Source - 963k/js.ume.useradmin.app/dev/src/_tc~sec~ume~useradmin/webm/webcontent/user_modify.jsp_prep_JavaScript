    function setSlct(slct) {
        for(var i=0; i<slct.options.length; i++) {
            if ( slct.options[i].value != "") {
              slct.options[i].selected = true;
            } else {
              slct.options[i].selected = false;
            }
        }
    }


















































function onSubmit____1530181465() {setSlctRoles();}

















































































 